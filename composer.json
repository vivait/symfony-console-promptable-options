{
  "name": "vivait/symfony-console-promptable-options",
  "description": "Allows you to specify options to be asked for via questions using the Symfony console component.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Matt Brunt",
      "email": "matt.brunt@vivait.co.uk"
    }
  ],
  "autoload": {
    "psr-4": {
      "Vivait\\PromptableOptions\\": "src/",
      "Vivait\\PromptableOptions\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=5.5.9",
    "symfony/console": "^2.8|^3.0",
    "symfony/options-resolver": "^3.2"
  },
  "require-dev": {
    "behat/behat": "^3.3",
    "symfony/process": "^3.2",
    "phpunit/phpunit": "^5.7"
  }
